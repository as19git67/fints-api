{
  "name": "fints-api",
  "version": "2.0.0",
  "description": "A FinTS/HBCI API based on fints npm module, implemented in typescript",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "postinstall": "npm run build",
    "generate": "cxsd -t generated https://raw.githubusercontent.com/hbci4j/hbci4java/master/src/main/resources/pain.001.001.03.xsd",
    "build": "tsc -p ./tsconfig-build.json",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "fints",
    "hbci",
    "banking"
  ],
  "author": "Anton Schegg",
  "license": "MIT",
  "devDependencies": {
    "cxsd": "^0.1.1",
    "tslint": "^5.20.1"
  },
  "dependencies": {
    "typescript": "^4.7.4",
    "@types/node": "^18.0.1",
    "date-fns": "^2.28.0",
    "fast-xml-parser": "^4.0.8",
    "iconv-lite": "^0.6.3",
    "isomorphic-fetch": "^3.0.0",
    "mt940-js": "^1.0.0",
    "winston": "^3.8.1"
  }
}
